{
    "name": "vscode-git-blame-show",
    "displayName": "vscode-git-blame-show",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.gitBlame",
        "onCommand:extension.gitShow",
        "onCommand:extension.gitShowFile"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.gitBlame",
                "title": "Git Blame"
            },
            {
                "command": "extension.gitShow",
                "title": "Git Show"
            },
            {
                "command": "extension.gitShowFile",
                "title": "Git Show File"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "vscode-git-blame-show",
                    "title": "vscode-git-blame-show",
                    "icon": ""
                }
            ]
        },
        "views": {
            "vscode-git-blame-show": [
                {
                "id": "vscode-git-blame-show-node",
                "name": "vscode-git-blame-show-node",
                "when": "explorer"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "yarn run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
